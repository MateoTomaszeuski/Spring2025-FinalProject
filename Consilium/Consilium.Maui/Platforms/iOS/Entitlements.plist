<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <!-- These entitlements are safe and common for development builds -->

  <!-- Allow JIT (e.g. for SkiaSharp, MAUI internals, debugging) -->
  <key>com.apple.security.cs.allow-jit</key>
  <true/>

  <!-- Allow loading unsigned libraries (debug-only workaround) -->
  <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
  <true/>

  <!-- Allow dynamic loading from outside the app bundle -->
  <key>com.apple.security.cs.disable-library-validation</key>
  <true/>
</dict>
</plist>
